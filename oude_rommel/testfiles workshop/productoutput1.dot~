digraph automaton {
	rankdir=LR
	start [style=invisible]
	start -> "A,E" [label=start]
	"C,G" [shape=doublecircle]
    "A,E" -> "B,E" [label="a"]
    "A,E" -> "A,F" [label="b"]
    "A,F" -> "B,F" [label="a"]
    "A,F" -> "A,G" [label="b"]
    "A,G" -> "B,G" [label="a"]
    "A,G" -> "A,G" [label="b"]
    "B,E" -> "C,E" [label="a"]
    "B,E" -> "B,F" [label="b"]
    "B,F" -> "C,F" [label="a"]
    "B,F" -> "B,G" [label="b"]
    "B,G" -> "C,G" [label="a"]
    "B,G" -> "B,G" [label="b"]
    "C,E" -> "D,E" [label="a"]
    "C,E" -> "C,F" [label="b"]
    "C,F" -> "D,F" [label="a"]
    "C,F" -> "C,G" [label="b"]
    "C,G" -> "D,G" [label="a"]
    "C,G" -> "C,G" [label="b"]
    "D,E" -> "D,E" [label="a"]
    "D,E" -> "D,F" [label="b"]
    "D,F" -> "D,F" [label="a"]
    "D,F" -> "D,G" [label="b"]
    "D,G" -> "D,G" [label="a"]
    "D,G" -> "D,G" [label="b"]
}
