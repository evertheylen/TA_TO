<?xml version="1.0" ?>
<AUTOMATON name = "AUTOMAAT1">
	<ALPHABET>
		<SYMBOL>0</SYMBOL>
		<SYMBOL>1</SYMBOL>
		<SYMBOL>2</SYMBOL>
	</ALPHABET>
	
	<STATES>
		<START>
			<STATE name = "A"></STATE>
		</START>
		
		<STATE name = "B"></STATE>
		
		<END>
			<STATE name = "C"></STATE>
		</END>
	</STATES>
	
	<TRANSITIONS>
		<TRANSITION from="A" symbol="0" to="A"></TRANSITION>
		<TRANSITION from="A" symbol="1" to="B"></TRANSITION>
		<TRANSITION from="A" symbol="2" to="C"></TRANSITION>
		...
	</TRANSITIONS>
</AUTOMATON>
