digraph automaton {
    node [shape=none]; start;
    node [shape=doublecircle]; "{D}";
    node [shape=circle];
    start -> "{A}";
    "{A}" -> "{B}" [label="0"];
    "{A}" -> "{A}" [label="1"];
    "{B}" -> "{A}" [label="0"];
    "{B}" -> "{C}" [label="1"];
    "{C}" -> "{D}" [label="0"];
    "{C}" -> "{B}" [label="1"];
    "{D}" -> "{D}" [label="0"];
    "{D}" -> "{A}" [label="1"];
}
