digraph automaton {
	rankdir=LR
	start [style=invisible]
	start -> "A,C" [label=start]
	"A,E" [shape=doublecircle]
    "A,C" -> "B,C" [label="0"]
    "A,C" -> "B,D" [label="1"]
    "A,D" -> "B,C" [label="0"]
    "A,D" -> "B,E" [label="1"]
    "A,E" -> "B,C" [label="0"]
    "A,E" -> "B,E" [label="1"]
    "B,C" -> "A,C" [label="0"]
    "B,C" -> "B,D" [label="1"]    
    "B,D" -> "A,C" [label="0"]
    "B,D" -> "B,E" [label="1"]
    "B,E" -> "A,C" [label="0"]
    "B,E" -> "B,E" [label="1"]
}
