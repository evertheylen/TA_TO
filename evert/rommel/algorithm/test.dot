    "{q3, q3}" -> "error" [label="+"];
    "{q3, q3}" -> "error" [label="-"];
    "{q3, q3}" -> "error" [label="."];
    "{q3, q3}" -> "error" [label="0"];
    "{q3, q3}" -> "error" [label="1"];
