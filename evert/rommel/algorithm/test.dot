    "{q1}" -> "{q2, q4, q4}" [label="1"];
    "{q0}" -> "{q1, q1}" [label="-"];
    "{q0}" -> "error" [label="."];
    "{q0}" -> "error" [label="0"];
    "{q0}" -> "error" [label="1"];
