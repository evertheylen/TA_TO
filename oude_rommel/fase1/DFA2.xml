<?xml version="1.0" ?>
<!-- based on example 2.18 on page 73 -->

<AUTOMATON>
	<ALPHABET>
		<SYMBOL>a</SYMBOL>
		<SYMBOL>b</SYMBOL>
	</ALPHABET>
	
	<STATES>
		<STATE name = "C" start="true"></STATE>
		<STATE name = "D"></STATE>
		<STATE name = "E" end="true"></STATE>
	</STATES>
	
	<TRANSITIONS>
		<TRANSITION from="C" symbol="a" to="D"></TRANSITION>
		<TRANSITION from="C" symbol="b" to="E"></TRANSITION>
		<TRANSITION from="D" symbol="a" to="E"></TRANSITION>
		<TRANSITION from="D" symbol="b" to="C"></TRANSITION>
		<TRANSITION from="E" symbol="a" to="C"></TRANSITION>
		<TRANSITION from="E" symbol="b" to="D"></TRANSITION>
	</TRANSITIONS>
</AUTOMATON>
